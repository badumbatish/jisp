s_expression = { atom_symbol | list | "(" ~ s_expression ~ "." ~ s_expression ~ ")" }

list = { "(" ~ s_expression ~ s_expression* ~ ")" }

atom_symbol = { letter ~ atom_part }
atom_part   = { (empty | letter ~ atom_part | number ~ atom_part) }
letter      = { ASCII_ALPHA_LOWER }
number      = { ASCII_DIGIT }

empty = { " " }
